'scopeName': 'source.spice'
'name': 'SPICE'
'fileTypes': ['CIR']
'patterns': [
  {
    'match': '\\*.*$'
    'name': 'comment'
  },
  {
    'match': '^\\..*$'
    'name': 'keyword'
  },
  {
    'match': '^([rRcClLDd][^ ]*)\\s+([^ ]*)\\s+([^ ]*)\\s+([^ ]*)$'
    'captures':
      '1':
        'name': 'variable'
      '2':
        'name': 'node.constant'
      '3':
        'name': 'node.constant'
      '4':
        'name': 'variable'
  },
  {
    'match': '\\b(tran|echo|ac)\\b'
    'captures':
      '1':
        'name': 'entity.name.function.source'
  },
  {
    'match': '\\d+(ms|us|Hz|n|Meg|K|kHz|H|V)'
    'name': 'constant'
  },
  {
    'begin': '"'
    'beginCaptures':
      '0':
        'name': 'punctuation.definition.string.begin'
    'end': '"'
    'endCaptures':
      '0':
        'name': 'punctuation.definition.string.end'
    'name': 'string.quoted.double'
    'patterns': [
      {
        'match': '\\$[A-Za-z]*'
        'name': 'variable'
      }
    ]
  }
]
